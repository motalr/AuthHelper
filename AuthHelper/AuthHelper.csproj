<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>motalr.AuthHelper</PackageId>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Ryan Motal</Authors>
    <Company>Ryan Motal</Company>
    <Product>motalr.AuthHelper</Product>
    <Description>Simple helper for authenticating users using Token in .NET Web API</Description>
